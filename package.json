{
  "name": "jformer",
  "version": "2.0.0",
  "description": "基于vjform 二次开发，简化了 json 定义方式且支持 vjform 全部功能",
  "main": "dist/jformer.umd.min.js",
  "module": "dist/jformer.common.js",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "dist": "vue-cli-service build --target lib ./lib/index.js",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fyl080801/jformer.git"
  },
  "keywords": [
    "json",
    "form",
    "dynamic form"
  ],
  "author": "fyl080801",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fyl080801/jformer/issues"
  },
  "homepage": "https://github.com/fyl080801/jformer#readme",
  "dependencies": {
    "jpresent-transform-modern": "^1.1.0",
    "lodash-es": "^4.17.15",
    "vjform": "^2.1.0-beta.1",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "element-ui": "^2.13.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "sass-loader": "^10.0.2",
    "vue-router": "^3.2.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
